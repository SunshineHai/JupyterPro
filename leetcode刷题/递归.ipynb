   "source": [
    "# 累加：\n",
    "def factoral(n : int) -> int:\n",
    "    sum = 1\n",
    "    while n > 0:\n",
    "        sum *= n\n",
    "        n -= 1\n",
    "    return sum\n",
    "factoral(5)"
   ]
