    "# 递归：求5的阶乘\n",
    "def factoral(n : int) -> int:\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factoral(n-1)\n",
    "factoral(5)"
   ]
  },
